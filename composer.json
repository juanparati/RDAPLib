{
    "name": "juanparati/rdap-lib",
    "description": "A PHP RDAP client library",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Juan Lago",
            "email": "juanparati@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.1",
        "ext-json": "*",
        "guzzlehttp/guzzle": "~6.0",
        "psr/http-client": "^1.0",
        "psr/http-factory": "^1.0",
        "ricardofiorani/guzzle-psr18-adapter": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0"
    },
    "autoload": {
        "psr-4": {
            "Juanparati\\RDAPLib\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Juanparati\\RDAPLib\\Tests\\": "tests/Test/"
        }
    }
}
